<h1>Listing registrations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Operator name</th>
    <th>Operator email</th>
    <th>Loc str</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Owner</th>
    <th>Created by</th>
    <th>Updated by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @registrations.each do |registration| %>
  <tr>
    <td><%= registration.name %></td>
    <td><%= registration.operator_name %></td>
    <td><%= registration.operator_email %></td>
    <td><%= registration.loc_str %></td>
    <td><%= registration.latitude %></td>
    <td><%= registration.longitude %></td>
    <td><%= registration.owner_id %></td>
    <td><%= registration.created_by %></td>
    <td><%= registration.updated_by %></td>
    <td><%= link_to 'Show', registration %></td>
    <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
    <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
